[32m+[m
[32m+[m[32m/*[m
[32m+[m[32m    Welcome to your first dbt model![m
[32m+[m[32m    Did you know that you can also configure models directly within SQL files?[m
[32m+[m[32m    This will override configurations stated in dbt_project.yml[m
[32m+[m
[32m+[m[32m    Try changing "table" to "view" below[m
[32m+[m[32m*/[m
[32m+[m
[32m+[m[32m{{ config(materialized='table') }}[m
[32m+[m
[32m+[m[32mwith source_data as ([m
[32m+[m
[32m+[m[32m    select 1 as id[m
[32m+[m[32m    union all[m
[32m+[m[32m    select null as id[m
[32m+[m
[32m+[m[32m)[m
[32m+[m
[32m+[m[32mselect *[m
[32m+[m[32mfrom source_data[m
[32m+[m
[32m+[m[32m/*[m
[32m+[m[32m    Uncomment the line below to remove records with null `id` values[m
[32m+[m[32m*/[m
[32m+[m
[32m+[m[32m-- where id is not null[m
